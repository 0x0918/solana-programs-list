{
  "name": "native-account-data",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "dependencies": {
    "@solana/web3.js": "^1.47.3",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/bn.js": "^5.1.0",
    "solana-bankrun": "^0.3.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "test": "bun test tests/account-data.test.ts --timeout 1000000",
    "test:watch": "bun test tests/account-data.test.ts --watch",
    "build": "cargo build-sbf --manifest-path=./account-data/Cargo.toml --sbf-out-dir=./account-data/target/so",
    "build-and-test": "cargo build-sbf --manifest-path=./account-data/Cargo.toml --sbf-out-dir=./tests/fixtures && bun run test",
    "deploy": "solana program deploy ./account-data/target/so/account_data.so"
  }
}
